<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AboveOurHeads.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AboveOurHeads.Views"
    Title="AboveOurHeads">

    <TabBar>
        
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

        <ShellContent
            Title="Tracker"
            Icon="track.png"
            ContentTemplate="{DataTemplate local:SatelliteTrackingPage}"
            Route="Traker" />

    </TabBar>
</Shell>
